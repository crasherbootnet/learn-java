<project name="SampleBuid" basedir=".">
    <target name="build">
        <exec executable="cmd">
            <arg value="/c"/>
            <arg value="mvn"/>
            <arg value="clean"/>
            <arg value="install"/>
        </exec>
    </target>
    <target name="exec">
        <exec dir="." executable="cmd">
            <arg line="/c mvn clean install"/>
        </exec>
    </target>
</project>